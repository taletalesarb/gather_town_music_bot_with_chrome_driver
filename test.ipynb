    "def kill(processs):\n",
    "    try:\n",
    "        proc_pid = processs.pid\n",
    "        process = psutil.Process(proc_pid)\n",
    "        for proc in process.children(recursive=True):\n",
    "            proc.kill()\n",
    "        process.kill()\n",
    "    except:\n",
    "        print(\"kill error\")"
   ]
